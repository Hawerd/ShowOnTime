        <column type="ro"   id="Scheduled"         width="80"      >Elemento Agendado    </column>
        <column type="ro"   id="CodeOfEvent"       width="80"      >Codigo de Evento     </column>
        <column type="ro"   id="Active"            width="80"      >Elemento Activo      </column>
        <column type='ro'   id='method'            hidden="true"   >method               </column>	
        <column type='ro'   id='op'                hidden="true"   >op		         </column>
        <afterInit> 	
            <call command="attachHeader"> 	
                <param>#text_filter,#text_filter,#select_filter,#select_filter,#select_filter,#select_filter,#text_filter,#select_filter</param>	
            </call>
        </afterInit>
    </head>
</rows>
