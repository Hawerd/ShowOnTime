<rows>
    <head>
        <column type="ro"   id="CodeOfStructure"   width="80"      >Codigo de Estructura </column>
        <column type="ro"   id="ElementName"       width="210"     >Nombre               </column>
        <column type="ro"   id="ElementBrand"      width="210"     >Marca                </column>
        <column type="ro"   id="Reference"         width="210"     >Referencia           </column>
